                <label for="Linkedin_URL" class="control-label">Linkedin URL</label>
                <input for="Linkedin_URL" class="form-control" @bind="@objCI.Linkedin_URL" />
            </div>
            @{
                var brunch = new List<string> { "Head Office", "Principal Branch", "SKB Branch", "Bogura Cantt. Branch", "Cumilla Cantt. Branch", "Chattogram Cantt. Branch", "Rangpur Cantt. Branch", "Jashore Cantt. Branch" };
            }

            <div class="form-group">
                <label asp-for="TBL_Brunch">TBL Brunch: </label>
                <select asp-for="TBL_Brunch" class="form-control">
                    <option value="">-- Select brunch --</option>
                    @foreach (var TBL_Brunch in brunch)
                    {
                        <option value="@TBL_Brunch">@TBL_Brunch</option>
                    }
                </select>
            </div>
            @{
                var department = new List<string> { "Finance ", "Human resources", "Operations management", "IT" };
            }

            <div class="form-group">
                <label asp-for="TBL_Department">TBL Department: </label>
                <select asp-for="TBL_Department" class="form-control">
