                    }
                </select>
            </div>

        </div>
    </div>



    <div class="row">
        <div class="col-md-4">
            <div class="form-group">
                <input type="button" @onclick="@UpdateIntern" class="btn btn-primary" value="Save" />
                <input type="button" @onclick="@Cancel" class="btn btn-primary" value="Cancel" />
            </div>
        </div>
    </div>
</form>
@code {
    [Parameter]
    public string CurretnID { get; set; }
    InternInfo objCI = new InternInfo();

    private DateTime DOB;
    

    protected override async Task OnInitializedAsync()
    {
        objCI = await Task.Run(() => ObjCustomService.GetInternById(Convert.ToInt32(CurretnID)));
        DOB = objCI.DOB;
    }
    protected void UpdateIntern()
    {
        ObjCustomService.UpdateIntern(objCI);
        NavigationManager.NavigateTo("Interns");
    }
    void Cancel()
    {
        NavigationManager.NavigateTo("Interns");
    }
}

