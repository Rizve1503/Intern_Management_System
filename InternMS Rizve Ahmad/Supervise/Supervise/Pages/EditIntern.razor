@page "/EditIntern/{CurretnID}"
@using Supervise.Data
@using System;
@inject InternService ObjCustomService
@inject NavigationManager NavigationManager
<h2>Edit Intern</h2>
<hr />
<form>
    <div class="row">
        <div class="col-md-8">
            <div class="form-group">
                <label for="Name" class="control-label">Name</label>
                <input for="Name" class="form-control" @bind="@objCI.Name" placeholder="Full Name" />
            </div>
            <div class="form-group">
                <label for="Password" class="control-label">Password</label>
                <input for="Password" class="form-control" @bind="@objCI.Password" />
            </div>
            <div class="form-group">
                <label for="Email" class="control-label">Email</label>
                <input for="Email" class="form-control" @bind="@objCI.Email" placeholder="abc@xyz.com" />
            </div>
            <div class="form-group">
                <label for="Phone" class="control-label">Phone Number</label>
                <input for="Phone" class="form-control" @bind="@objCI.Phone" />
            </div>
            <div class="form-group">
                <label for="NID" class="control-label">NID</label>
                <input for="NID" class="form-control" @bind="@objCI.NID" />
            </div>
            <div class="form-group">
                <label asp-for="Gender" class="control-label">Gender</label>
                <select @bind="@objCI.Gender" class="form-control">
                    <option value="@objCI.Gender">@objCI.Gender</option>
                    <option value="Male">Male</option>
                    <option value="Female">Female</option>
                    <option value="Other">Other</option>
                </select>
            </div>
