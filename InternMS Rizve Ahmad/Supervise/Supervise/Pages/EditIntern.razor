            <h3>Present Address</h3>

            <div class="form-group">
            <label asp-for="Present_division">Present Division</label>
            <select asp-for="Present_division" class="form-control">
            <option value="@objCI.Present_division">@objCI.Present_division</option>
            @foreach (var division in divisions)
            {
            <option value="@division">@division</option>
            }
            </select>
            </div>

            <div class="form-group">
            <label asp-for="Present_district">Present District</label>
            <select asp-for="Present_district" class="form-control">
            <option value="@objCI.Present_district">@objCI.Present_district</option>
            @if (!string.IsNullOrEmpty(objCI.Present_division))
            {
            foreach (var district in districts[objCI.Present_division])
            {
            <option value="@district">@district</option>
            }
            }
            </select>
            </div>

            <div class="form-group">
            <label asp-for="Present_thanna">Present Thana</label>
            <select asp-for="Present_thanna" class="form-control">
            <option value="@objCI.Present_thanna">@objCI.Present_thanna</option>
            @if (!string.IsNullOrEmpty(objCI.Present_district))
            {
            foreach (var thana in thanas[objCI.Present_district])
            {
            <option value="@thana">@thana</option>
            }
            }
            </select>
            </div>
